{
  "<website-content-list-node>": {
    "template": "./content-list.marko",
    "@node": "object",
    "@display-image": {
      "type": "boolean",
      "default-value": true
    },
    "@with-teaser": {
      "type": "boolean",
      "default-value": false
    }
  },
  "<website-content-card-node>": {
    "template": "./content-card.marko",
    "@node": "object",
    "@title-modifiers": "array",
    "@display-image": {
      "type": "boolean",
      "default-value": true
    },
    "@with-teaser": {
      "type": "boolean",
      "default-value": true
    }
  }
}
