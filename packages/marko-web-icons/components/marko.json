{
  "<marko-web-icon>": {
    "template": "./icon",
    "@name": {
      "type": "string",
      "required": true
    },
    "@block-name": {
      "type": "string",
      "default-value": "marko-web-icon"
    },
    "@tag": {
      "type": "string",
      "default-value": "span"
    },
    "@modifiers": "array",
    "@class": "string",
    "@attrs": "object"
  }
}
