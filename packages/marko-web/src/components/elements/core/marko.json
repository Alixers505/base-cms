{
  "tags": {
    "cms-text-element": {
      "template": "./text.marko"
    },
    "cms-html-element": {
      "template": "./html.marko"
    },
    "cms-image-element": {
      "template": "./image.marko"
    },
    "cms-date-element": {
      "template": "./date.marko"
    },
    "cms-link-element": {
      "template": "./link.marko"
    },
    "cms-load-more-element": {
      "template": "./load-more.marko",
      "@append-to": {
        "type": "string",
        "required": true
      },
      "@block-name": "string",
      "@params": {
        "type": "object",
        "required": true
      },
      "@page-info": {
        "type": "object",
        "required": true
      },
      "@label": {
        "type": "string",
        "default-value": "Load More"
      },
      "@button-class-obj": "object",
      "@wrapping-class-obj": "object"
    }
  }
}
