{
  "tags": {
    "cms-gtm-container-init": {
      "template": "./container-init.marko",
      "@container-id": {
        "type": "expression",
        "required": true
      },
      "@name": {
        "type": "string",
        "required": true,
        "default-value": "dataLayer"
      }
    },
    "cms-gtm-container-push": {
      "template": "./container-push.marko",
      "@name": {
        "type": "expression",
        "required": true,
        "default-value": "dataLayer"
      },
      "@data": {
        "type": "object",
        "required": true
      }
    },
    "cms-gtm-container-start": {
      "template": "./container-start.marko",
      "@name": {
        "type": "expression",
        "required": true,
        "default-value": "dataLayer"
      }
    },
    "cms-gtm-track-inview-event": {
      "template": "./track-inview-event.marko"
    }
  }
}
