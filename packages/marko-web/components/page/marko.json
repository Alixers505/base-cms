{
  "<marko-web-page-container>": {
    "template": "./container.marko",
    "@tag": {
      "type": "string",
      "default-value": "main"
    },
    "@for": "string",
    "@id": "string",
    "@type": "string",
    "@class": "string",
    "@attrs": "object"
  },
  "<cms-page-title>": {
    "template": "./title.marko"
  },
  "<marko-web-page-rel-canonical>": {
    "template": "./rel-canonical.marko",
    "@path": "string"
  },
  "<cms-page-description>": {
    "template": "./description.marko"
  },
  "<marko-web-page-metadata>": {
    "template": "./metadata/index.marko",
    "@title": "string",
    "@description": "string",
    "@canonical-path": "string",
    "@image-src": "string"
  },
  "<marko-web-page-content-metadata>": {
    "template": "./metadata/content.marko",
    "@type": "string",
    "@title": "string",
    "@description": "string",
    "@canonical-path": "string",
    "@image-src": "string",
    "@published-date": "date",
    "@updated-date": "date",
    "@expires-date": "date"
  }
}
