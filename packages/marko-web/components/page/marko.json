{
  "taglib-imports": [
    "./layouts/marko.json"
  ],
  "<marko-web-page-container>": {
    "template": "./container.marko",
    "@tag": {
      "type": "string",
      "default-value": "main"
    },
    "@for": "string",
    "@id": "string",
    "@type": "string",
    "@class": "string",
    "@attrs": "object",
    "@modifiers": "array"
  },
  "<marko-web-page-wrapper>": {
    "template": "./wrapper.marko",
    "@sections <section>[]": {
      "@tag": {
        "type": "string",
        "default-value": "div"
      },
      "@modifiers": "array",
      "@class": "string",
      "@attrs": "object"
    },
    "@block-name": {
      "type": "string",
      "default-value": "page-wrapper"
    },
    "@tag": {
      "type": "string",
      "default-value": "div"
    },
    "@modifiers": "array",
    "@class": "string",
    "@attrs": "object"
  },
  "<marko-web-page-title>": {
    "template": "./title.marko",
    "@value": "string",
    "@concat-with": {
      "type": "string",
      "default-value": "|"
    }
  },
  "<marko-web-page-rel-canonical>": {
    "template": "./rel-canonical.marko",
    "@path": "string"
  },
  "<marko-web-page-description>": {
    "template": "./description.marko",
    "@value": "string"
  },
  "<marko-web-page-metadata>": {
    "template": "./metadata/index.marko",
    "@title": "string",
    "@description": "string",
    "@canonical-path": "string",
    "@image-src": "string"
  },
  "<marko-web-page-metadata-content>": {
    "template": "./metadata/content.marko",
    "@id": "number"
  },
  "<marko-web-page-metadata-website-section>": {
    "template": "./metadata/website-section.marko",
    "@alias": "string"
  },
  "<marko-web-page-metadata-dynamic-page>": {
    "template": "./metadata/dynamic-page.marko",
    "@alias": "string"
  }
}
