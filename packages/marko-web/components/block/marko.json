{
  "<marko-web-block-page-image>": {
    "template": "./page-image.marko",
    "@obj": "object",
    "@width": {
      "type": "number",
      "default-value": 320
    },
    "@block-name": {
      "type": "string",
      "default-value": "page-image"
    },
    "@lazyload": {
      "type": "boolean",
      "default-value": true
    },
    "@with-caption": {
      "type": "boolean",
      "default-value": true
    },
    "@with-credit": {
      "type": "boolean",
      "default-value": true
    }
  }
}
