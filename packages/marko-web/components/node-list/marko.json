{
  "<marko-web-node-list>": {
    "template": "./index.marko",
    "<header>": {},
    "<footer>": {},
    "<body>": {},
    "<render>": {},
    "@block-name": {
      "type": "string",
      "default-value": "node-list"
    },
    "@tag": {
      "type": "string",
      "default-value": "div"
    },
    "@nodes": "array",
    "@justified": {
      "type": "boolean",
      "default-value": false
    },
    "@inner-justified": {
      "type": "boolean",
      "default-value": false
    },
    "@flush":  {
      "type": "boolean",
      "default-value": false
    },
    "@full-height": {
      "type": "boolean",
      "default-value": false
    },
    "@collapsible": {
      "type": "boolean",
      "default-value": true
    },
    "@class": "string",
    "@modifiers": "array",
    "@attrs": "object"
  },
  "<marko-web-node-list-render>": {
    "template": "./render.marko"
  },
  "<marko-web-node-list-element>": {
    "template": "./element.marko",
    "@name": "string",
    "@tag": {
      "type": "string",
      "default-value": "div"
    },
    "@block-name": {
      "type": "string",
      "default-value": "node-list"
    },
    "@modifiers": "array",
    "@attrs": "object"
  }
}
