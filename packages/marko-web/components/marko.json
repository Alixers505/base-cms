{
  "taglib-imports": [
    "./element/marko.json",
    "./elements/marko.json",
    "./load-more/marko.json",
    "./node/marko.json",
    "./node-list/marko.json",
    "./page/marko.json",
    "./queries/marko.json"
  ],
  "<marko-web-browser-component>": {
    "template": "./browser-component.marko",
    "@name": {
      "type": "string",
      "required": true
    },
    "@props": "object"
  },
  "<marko-web-document>": {
    "template": "./document/index.marko",
    "<head>": {},
    "<before-container>": {},
    "<after-container>": {},
    "@dir": {
      "type": "string",
      "default-value": "ltr"
    },
    "@lang": "string",
    "@viewport": {
      "type": "string",
      "default-value": "width=device-width, initial-scale=1, shrink-to-fit=no"
    },
    "@with-container": {
      "type": "boolean",
      "default-value": true
    },
    "@container": "object"
  },
  "<marko-web-document-container>": {
    "template": "./document/container.marko",
    "@block-name": {
      "type": "string",
      "default-value": "document-container"
    },
    "@tag": {
      "type": "string",
      "default-value": "div"
    },
    "@modifiers": "array",
    "@class": "string",
    "@attrs": "object"
  }
}
