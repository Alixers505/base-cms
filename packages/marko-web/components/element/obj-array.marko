<marko-web-obj|{ value, classNames }|
  type=input.type
  block-name=input.blockName
  obj=input.obj
  field=input.field
  class=input.class
  modifiers=input.modifiers
>
  <marko-web-array|{ value: v, index, length }|
    attrs=input.attrs
    tag=input.tag
    value=value
    class=classNames
  >
    <${input.renderBody} value=v index=index length=length />
  </marko-web-array>
</marko-web-obj>
