import extractRender from "../extract-render";

$ if (input.link === true) {
  input.link = { field: "digitalEditionUrl" };
}

<marko-web-obj-text
  type="magazine-issue"
  block-name=input.blockName
  obj=input.obj
  field="digitalEditionUrl"
  tag=input.tag
  class=input.class
  modifiers=input.modifiers
  attrs=input.attrs
  link=input.link
  ...extractRender(input)
/>
