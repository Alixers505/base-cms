import extractRender from "../extract-render";

<marko-web-obj-array|{ value }|
  type="content"
  block-name=input.blockName
  obj=input.obj
  field="sidebars"
  tag=input.tag
  class=input.class
  modifiers=input.modifiers
  attrs=input.attrs
>
  <marko-web-obj-text
    type="content"
    block-name=input.blockName
    obj={ sidebar: value }
    field="sidebar"
    html=true
    ...extractRender(input)
  />
</marko-web-obj-array>
