{
  "<gtm-init>": {
    "template": "./init.marko",
    "@container-id": "expression",
    "@name": {
      "type": "string",
      "default-value": "dataLayer"
    },
    "@push": "object",
    "@start": {
      "type": "boolean",
      "default-value": false
    }
  },
  "<gtm-start>": {
    "template": "./start.marko",
    "@name": {
      "type": "string",
      "default-value": "dataLayer"
    }
  },
  "<gtm-push>": {
    "template": "./push.marko",
    "@name": {
      "type": "string",
      "default-value": "dataLayer"
    },
    "@data": "object"
  },
  "<gtm-track-inview-event>": {
    "template": "./track-inview-event.marko"
  }
}
