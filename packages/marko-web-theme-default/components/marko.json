{
  "taglib-imports": [
    "./breadcrumbs/marko.json"
  ],
  "<marko-theme-default-page-contents>": {
    "template": "./page-contents.marko",
    "@tag": {
      "type": "string",
      "default-value": "div"
    },
    "@class": "string",
    "@modifiers": "@array",
    "@attrs": "object"
  },
  "<marko-theme-default-page-dates>": {
    "template": "./page-dates.marko",
    "@tag": {
      "type": "string",
      "default-value": "div"
    },
    "@class": "string",
    "@modifiers": "@array",
    "@attrs": "object"
  },
  "<marko-theme-default-page-attribution>": {
    "template": "./page-attribution.marko",
    "@tag": {
      "type": "string",
      "default-value": "div"
    },
    "@class": "string",
    "@modifiers": "@array",
    "@attrs": "object"
  },
  "<marko-theme-default-content-attribution>": {
    "template": "./content-attribution/index.marko",
    "@obj": "object",
    "@elements": {
      "type": "array",
      "default-value": ["byline", "authors", "contributors", "photographers", "source", "company", "sponsors"]
    },
    "@tag": {
      "type": "string",
      "default-value": "div"
    },
    "@class": "string",
    "@modifiers": "@array",
    "@attrs": "object"
  }
}
