$ const { req } = out.global;
$ const { identityX } = req;

<marko-web-identity-x-context|{ user, isEnabled }|>
  $ const props = {
    activeUser: user,
    endpoints: identityX.config.getEndpoints(),
  };
  <if(isEnabled)>
    <marko-web-browser-component name="IdentityXLogin" props=props />
  </if>
</marko-web-identity-x-context>
