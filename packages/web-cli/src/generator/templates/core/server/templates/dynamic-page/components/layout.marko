import { getAsObject } from '@base-cms/object-path';

$ const page = getAsObject(input, 'page');

<cms-document>
  <@head>
    <cms-page-metadata for="dynamic-page" ...page />
    <${input.head} />
  </@head>
  <cms-page-container for="dynamic-page" data=page>
    <${input.renderBody} />
  </cms-page-container>
</cms-document>
